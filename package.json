{
  "name": "fleek",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/fabzer0/Fleek.git",
  "author": "fabischolasi <fabischapeli97@gmail.com>",
  "license": "MIT",
  "scripts": {
    "pretest": "NODE_ENV=test yarn run db:rollmigrate",
    "watch:test": "yarn test -- -w",
    "nyc:report": "nyc --reporter=html --reporter=lcov --reporter=text",
    "test": "NODE_ENV=test yarn nyc:report mocha --exit './src/**/*.spec.js' --require @babel/register --timeout 10000",
    "start:dev": "NODE_ENV=development DEBUG=log nodemon --exec babel-node ./express/index",
    "db:seed": "node_modules/.bin/sequelize db:seed:all",
    "db:migrate": "node_modules/.bin/sequelize db:migrate",
    "db:rollback": "node_modules/.bin/sequelize db:migrate:undo:all",
    "db:rollmigrate": "yarn db:rollback && yarn db:migrate && yarn db:seed"
  },
  "dependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@babel/register": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "cls-hooked": "^4.2.2",
    "cors": "^2.8.5",
    "crypto-random-string": "^3.0.1",
    "debug": "^4.1.1",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "fancy-log": "^1.3.3",
    "jsonwebtoken": "^8.5.1",
    "logger": "^0.0.1",
    "mocha": "^6.2.0",
    "morgan": "^1.9.1",
    "nodemailer": "^6.3.0",
    "nyc": "^14.1.1",
    "pg": "^7.11.0",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.10.2",
    "sinon": "^7.3.2"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.5.5",
    "nodemon": "^1.19.1",
    "sequelize-cli": "^5.5.0",
    "supertest": "^4.0.2"
  }
}
